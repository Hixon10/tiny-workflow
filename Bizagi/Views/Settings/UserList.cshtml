@model IEnumerable<Bizagi.ViewModels.Settings.UserViewModel>

@{
    ViewBag.Title = "Пользователи системы";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.ActionLink("Изменить роль", "EditUserRoles", new { id = item.UserName })
        </td>
    </tr>
}

</table>
